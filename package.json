{
  "name": "pokedex",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "coverage": "vitest run --coverage.enabled --coverage.reporter=cobertura --coverage.all --coverage.src=src",
    "coverage:local": "vitest run --coverage.enabled --coverage.reporter=html --coverage.all --coverage.src=src",
    "update-types": "npx supabase gen types typescript --project-id \"hykrtxkdrrgsabjsuavx\" --schema public > src/lib/db/types.ts"
  },
  "devDependencies": {
    "@fast-check/vitest": "^0.0.4",
    "@playwright/test": "^1.32.3",
    "@skeletonlabs/skeleton": "^1.2.0",
    "@sveltejs/adapter-vercel": "^2.4.2",
    "@sveltejs/kit": "^1.15.7",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/ui": "^0.30.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.22",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "supabase": "^1.50.8",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "sveltekit-superforms": "^0.7.1",
    "tailwindcss": "^3.3.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.2",
    "vitest": "^0.30.1",
    "vitest-fetch-mock": "^0.2.2",
    "vitest-mock-extended": "^1.1.3",
    "web-image-gen": "^1.1.5",
    "zod": "^3.21.4",
    "zod-fast-check": "^0.9.1"
  },
  "type": "module",
  "dependencies": {
    "@supabase/supabase-js": "^2.20.0",
    "@vercel/analytics": "^0.1.11",
    "web-image-gen-svelte": "^1.0.5",
    "web-vitals": "^3.3.1"
  }
}
